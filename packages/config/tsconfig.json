{
  "compilerOptions": {
    "target": "ESNext",
    "useDefineForClassFields": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "strict": true,
    "jsx": "preserve",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "lib": [
      "ESNext",
      "DOM"
    ],
    "types": [],
    "skipLibCheck": true,
    "noEmit": false,
    "rootDir": "./",
    "baseUrl": "./"
  },
  "include": [
    "*.ts",
    "./calendar/*.ts",
  ],
  "exclude": []
}