{
  "compilerOptions": {
    "target": "ESNext",
    "useDefineForClassFields": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "strict": true,
    "jsx": "preserve",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "lib": [
      "ESNext",
      "DOM"
    ],
    "types": [
      "vite/client",
      "@task/env"
    ],
    "skipLibCheck": true,
    "noEmit": false,
    "rootDir": "./",
    "baseUrl": "./",
  },
  "include": [
    "main/**/*.ts",
    "renderer/**/*.ts",
    "interface/**/*.ts",
  ],
  "exclude": []
}